    ],
    "no-var": ["error"],
    "object-shorthand": [
      "error",
      "always",
      {
        "ignoreConstructors": false,
        "avoidQuotes": true
      }
    ],
    "prefer-numeric-literals": ["off"],
    "prefer-reflect": ["off"],
    "prefer-spread": ["error"],
    "require-yield": ["error"],
    "sort-imports": [
      "off",
      {
        "ignoreCase": false,
        "ignoreMemberSort": false,
