    ],
    "symbol-description": ["error"],
    "init-declarations": ["off"],
    "no-catch-shadow": ["off"],
    "no-delete-var": ["error"],
    "no-label-var": ["error"],
    "no-restricted-globals": ["off"],
    "no-shadow": ["error"],
    "no-shadow-restricted-names": ["error"],
    "no-undef-init": ["error"],
    "no-undefined": ["off"],
    "camelcase": [
